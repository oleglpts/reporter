    <report>
        <name>$_title1_ ($_title2_)</name>
        <styledef name="Subheaders">
            font: bold True; alignment: horiz centre;
            borders: left 1, top 1, bottom 1, right 1
        </styledef>
        <styledef name="Headers">font: bold True;</styledef>
        <literal col="0" row="0" stylename="Headers">Report:</literal>
        <literal col="1" row="0">$_title0_</literal>
        <literal col="0" row="1" stylename="Headers">From:</literal>
        <literal col="1" row="1">$_parm0_</literal>
        <literal col="2" row="1" stylename="Headers">To:</literal>
        <literal col="3" row="1">$_parm1_</literal>
        <literal col="4" row="1" stylename="Headers">Client:</literal>
        <literal col="5" row="1">$_title1_</literal>
        <literal col="6" row="1" stylename="Headers">Bank:</literal>
        <literal col="7" row="1">$_title2_</literal>
        <literal col="0" row="3" stylename="Subheaders">Currency</literal>
        <literal col="1" row="3" stylename="Subheaders">Type</literal>
        <literal col="2" row="3" stylename="Subheaders">Number</literal>
        <literal col="3" row="3" stylename="Subheaders">Amount</literal>
        <literal col="4" row="3" stylename="Subheaders">Captured</literal>
        <literal col="5" row="3" stylename="Subheaders">TrFee</literal>
        <literal col="6" row="3" stylename="Subheaders">AuFee</literal>
        <literal col="7" row="3" stylename="Subheaders">Reserve</literal>
        <literal col="8" row="3" stylename="Subheaders">MnFee</literal>
        <literal col="9" row="3" stylename="Subheaders">Total</literal>
        <sql>
            <request>
                SET TIME ZONE 0;
                SELECT ccurr, ctrans, camount, ccaptured, ctrfee::decimal(18,2), cauthfee, cdeposit::decimal(18,2),
                       cmonth, clastpay, captlastpay, cdeppay, captdeppay::decimal(18,2), rtrans, ramount,
                       rtrfee::decimal(18,2), rauthfee, cdecl, cdamount, cdecltrfee::decimal(18,2), cdeclfee,
                       frdtrans, frdamount, frdfee, chbtrans, chbamount, chbfee, cbrtrans, cbramount
                  FROM aps_statement_group_bank('$_parm0_', '$_parm1_', '{$_parm2_}', '$_parm3_')
            </request>
            <group step="10">
                <field col="0" name="Currency" row="5" header="no" stylename="Headers" format="@"/>
                <groupliteral col="1" name="Type" row="5" header="no" stylename="Headers">Debit</groupliteral>
                <field col="2" name="Number" row="5" header="no"/>
                <field col="3" name="Amount" row="5" header="no" format="0.00"/>
                <field col="4" name="Captured" row="5" header="no" format="0.00"/>
                <field col="5" name="TrFee" row="5" header="no" format="0.00"/>
                <field col="6" name="AuFee" row="5" header="no" format="0.00"/>
                <field col="7" name="Reserve" row="5" header="no" format="0.00"/>
                <field col="8" name="MnFee" row="5" header="no" format="0.00"/>
                <formula col="9" name="Total" row="5" header="no" format="0.00">
                    E$_cs-F$_cs-G$_cs-H$_cs-I$_cs
                </formula>
                <formula col="0" name="Currency" row="13" stylename="Headers" header="no">
                    INDIRECT("A" &amp; ($_ss+5))
                </formula>
                <groupliteral col="1" name="Type" row="6" header="no" stylename="Headers">Payments</groupliteral>
                <field col="2" name="Number" row="6" header="no"/>
                <field col="4" name="Captured" row="6" header="no" format="0.00"/>
                <groupliteral col="1" name="Type" row="7" header="no" stylename="Headers">Reserve refund</groupliteral>
                <field col="2" name="Number" row="7" header="no"/>
                <field col="4" name="Captured" row="7" header="no" format="0.00"/>
                <formula col="9" name="Total" row="7" header="no" format="0.00">E$_cs</formula>
                <groupliteral col="1" name="Type" row="8" header="no" stylename="Headers">Refund</groupliteral>
                <field col="2" name="Number" row="8" header="no"/>
                <field col="3" name="Amount" row="8" header="no" format="0.00"/>
                <field col="5" name="TrFee" row="8" header="no" format="0.00"/>
                <field col="6" name="AuFee" row="8" header="no" format="0.00"/>
                <formula col="9" name="Total" row="8" header="no" format="0.00">D$_cs+F$_cs+G$_cs</formula>
                <groupliteral col="1" name="Type" row="9" header="no" stylename="Headers">Declined</groupliteral>
                <field col="2" name="Number" row="9" header="no"/>
                <field col="3" name="Amount" row="9" header="no" format="0.00"/>
                <field col="5" name="TrFee" row="9" header="no" format="0.00"/>
                <field col="6" name="AuFee" row="9" header="no" format="0.00"/>
                <formula col="9" name="Total" row="9" header="no" format="0.00">F$_cs+G$_cs</formula>
                <groupliteral col="1" name="Type" row="10" header="no" stylename="Headers">Fraud</groupliteral>
                <field col="2" name="Number" row="10" header="no"/>
                <field col="3" name="Amount" row="10" header="no" format="0.00"/>
                <field col="5" name="TrFee" row="10" header="no" format="0.00"/>
                <formula col="9" name="Total" row="10" header="no" format="0.00">D$_cs+F$_cs</formula>
                <groupliteral col="1" name="Type" row="11" header="no" stylename="Headers">Chargeback</groupliteral>
                <field col="2" name="Number" row="11" header="no"/>
                <field col="3" name="Amount" row="11" header="no" format="0.00"/>
                <field col="5" name="TrFee" row="11" header="no" format="0.00"/>
                <formula col="9" name="Total" row="11" header="no" format="0.00">D$_cs+F$_cs</formula>
                <groupliteral col="1" name="Type" row="12" header="no" stylename="Headers">CHB refund</groupliteral>
                <field col="2" name="Number" row="12" header="no"/>
                <field col="3" name="Amount" row="12" header="no" format="0.00"/>
                <formula col="9" name="Total" row="12" header="no" format="0.00">D$_cs</formula>
            </group>
        </sql>
        <sql cycle="yes">
            <group step="10">
                <groupliteral col="1" row="13" stylename="Headers">To transfer</groupliteral>
                <formula col="9" name="Total" row="13" header="no" format="0.00">
                    INDIRECT("J" &amp; ($_ss+5))+
                    INDIRECT("J" &amp; ($_ss+6))+
                    INDIRECT("J" &amp; ($_ss+7))-
                    INDIRECT("J" &amp; ($_ss+8))-
                    INDIRECT("J" &amp; ($_ss+9))-
                    INDIRECT("J" &amp; ($_ss+10))-
                    INDIRECT("J" &amp; ($_ss+11))+
                    INDIRECT("J" &amp; ($_ss+12))
                </formula>
            </group>
        </sql>
    </report>
